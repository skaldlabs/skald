# Core Django Settings
SECRET_KEY=your-secret-key-here-generate-with-get-random-secret-key
DEBUG=false

# Database Configuration
DATABASE_URL=postgres://username:password@localhost:5432/skald2

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PUB_SUB_CHANNEL_NAME=process_memo

# AI/ML API Keys
VOYAGE_API_KEY=your-voyage-api-key
VOYAGE_EMBEDDING_MODEL=voyage-3-large
OPENAI_API_KEY=your-openai-api-key

# Optional: LangSmith (for tracing)
LANGSMITH_API_KEY=your-langsmith-api-key

# Email Service
RESEND_API_KEY=your-resend-api-key
EMAIL_DOMAIN=useskald.com
EMAIL_VERIFICATION_ENABLED=true

# Frontend Configuration
FRONTEND_URL=http://localhost:5173
APP_HOST=http://localhost:3000

# Stripe Payment Configuration
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret

# Analytics
POSTHOG_PUBLIC_API_KEY=your-posthog-key
POSTHOG_HOST=https://app.posthog.com

# AWS Configuration (Production only)
USE_SQS=false
SQS_QUEUE_URL=your-sqs-queue-url
AWS_REGION=us-east-2

# Logging Levels
DJANGO_LOG_LEVEL=WARNING
APP_LOG_LEVEL=DEBUG

# Security Settings (DO NOT set to true in production!)
DISABLE_AUTH=false
